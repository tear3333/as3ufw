package app.core.controller.startup {	import app.core.controller.ApplyURIChangeCommand;	import app.core.controller.ChangeURICommand;	import app.core.events.InitialDataServiceEvent;	import app.core.events.URIEvent;	import org.robotlegs.base.ContextEvent;	import org.robotlegs.mvcs.Command;	public class PrepControllerCommand extends Command {		[Inject]		public var event : ContextEvent;		public function PrepControllerCommand() {		}		override public function execute() : void {			mapStartupControllers();			mapApplicationControllers();		}		private function mapStartupControllers() : void {			commandMap.mapEvent(InitialDataServiceEvent.DATA_LOADED, ApplyInitialDataCommand, InitialDataServiceEvent, true);		}		private function mapApplicationControllers() : void {			commandMap.mapEvent(URIEvent.URI_CHANGED, ApplyURIChangeCommand, URIEvent);			commandMap.mapEvent(URIEvent.CHANGE_URI, ChangeURICommand, URIEvent);		}	}}