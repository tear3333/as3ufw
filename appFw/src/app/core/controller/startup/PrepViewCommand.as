package app.core.controller.startup {	import app.view.ApplicationView;	import app.view.mediators.ApplicationMediator;	import org.robotlegs.base.ContextEvent;	import org.robotlegs.mvcs.Command;	public class PrepViewCommand extends Command {		[Inject] 		public var event : ContextEvent;		public function PrepViewCommand() {		}		override public function execute() : void {			initMediators();		}		private function initMediators() : void {			mediatorMap.mapView(ApplicationView, ApplicationMediator);			log('Map application specific mediators!');		}	}}