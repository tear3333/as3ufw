package app.core.events {	import flash.events.Event;	public class URIEvent extends Event {		public static const URI_CHANGED : String = "URIChanged";		public static const CHANGE_URI : String = "ChangeURI";		public var uri : String;		public function URIEvent(type : String, uri : String = null) {			super(type);			this.uri = uri;		}		override public function clone() : Event {			var event : URIEvent = new URIEvent(type);			event.uri = uri;			return event;		}	}}