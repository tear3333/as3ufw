package app.core.controller.startup {	import app.core.services.InitialDataService;	import app.core.view.ApplicationView;	import org.robotlegs.base.ContextEvent;	import org.robotlegs.mvcs.Command;	public class StartupCommand extends Command {		[Inject] 		public var startupService : InitialDataService;		public function StartupCommand() {		}		override public function execute() : void {			contextView.addChild(new ApplicationView());			startupService.loadInitialData();			dispatch(new ContextEvent(ContextEvent.STARTUP_COMPLETE));		}	}}